<?xml version = "1.0" encoding = "utf-8"?>
<!-- 河北一卡通终端规范 -->
<config>
<!-- 公共配置 -->
<!-- 反馈报文长度占位 -->
<respLen>2</respLen>
<!-- 签到配置 -->
<trans code="sign">
	<head>
		<field index="1" alias="TPDU"  length="10" lengthType="FIX" mode="BCD" value="6000000000"/>
		<field index="2" alias="HEAD"  length="12" lengthType="FIX" mode="BCD" value="000000000000"/>
	</head>
	<type  alias="TYPE"  length="4" lengthType="FIX" mode="BCD" value="0900"/> 
	<body>
		<field index="3" alias="processCode"  length="6" lengthType="FIX" mode="BCD" req="M" resp="M" value="992051"/><!--交易处理码 -->
		<field index="11" alias="traceNo"  length="6" lengthType="FIX" mode="BCD" req="M" resp="M"/><!-- 受卡方系统跟踪号 -->
		<field index="22" alias="entryMode"  length="3" lengthType="FIX" mode="BCD" req="M" value="020"/><!-- 1位FLAG（类型）+2位版本号组成 FLAG（类型）为：充值签到为0-->
		<field index="39" alias="respCode"  length="2" lengthType="FIX" mode="ASC" resp="M"/><!-- 应答码 -->
		<field index="41" alias="terminalNo"  length="8" lengthType="FIX" mode="ASC" req="M" resp="M"/><!-- 终端号 -->
		<field index="60" alias="reserved60"  length="11" lengthType="LLLBCD" mode="BCD" req="M" resp="M"/><!-- 自定义域 -->
		<field index="63" alias="reserved63"  length="3" lengthType="LLLBCD" mode="ASC" req="M"/><!--操作员代码 -->
		<field index="64" alias="mac"  length="64" lengthType="FIX" mode="BIN" req="M" resp="C"/><!--MAC -->
	</body>
	<mac>3|11|22|39|41|60|63|64</mac>
</trans>
</config>