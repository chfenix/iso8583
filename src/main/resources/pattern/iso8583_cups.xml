		<field index="25" alias="conditionMode"  length="2" lengthType="FIX" mode="BCD" req="M" resp="M" value="00"/><!-- 服务点条件码 -->
		<field index="26" alias="pinCaptureCode"  length="2" lengthType="FIX" mode="BCD" req="C" value="06"/><!-- 服务点PIN获取码 -->
